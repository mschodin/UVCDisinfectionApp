{
  "expo": {
    "name": "UVCApp",
    "slug": "uvc-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/uvc-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#9900FF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.teammama.uvcappios",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.teammama.uvcappandroid",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/uvc-icon.png",
        "backgroundColor": "#9900FF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
